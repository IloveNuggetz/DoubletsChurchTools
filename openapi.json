                    "nationalitaetId": {
                        "type": "integer"
                    },
                    "familienstandNo": {
                        "type": "integer"
                    },
                    "hochzeitsdatum": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "erstkontakt": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "zugehoerig": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "eintrittsdatum": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "austrittsgrund": {
                        "type": "string"
                    },
                    "austrittsdatum": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "taufdatum": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "taufort": {
                        "type": "string"
                    },
                    "getauftdurch": {
                        "type": "string"
                    },
                    "ueberwiesenvon": {
                        "type": "string"
                    },
                    "ueberwiesennach": {
                        "type": "string"
                    },
                    "imageurl": {
                        "type": "string",
                        "nullable": true
                    },
                    "familyimageurl": {
                        "type": "string",
                        "nullable": true
                    },
                    "growpathId": {
                        "type": "integer",
                        "nullable": true
                    },
                    "letzteaenderung": {
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "aenderunguser": {
                        "type": "integer",
                        "nullable": true
                    },
                    "gev": {
                        "type": "integer",
                        "nullable": true
                    },
                    "person": {
                        "nullable": true,
